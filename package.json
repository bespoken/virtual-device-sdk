{
  "name": "virtual-device-sdk",
  "license": "Apache 2.0",
  "private": false,
  "main": "./lib/src/Index.js",
  "typings": "./lib/src/Index.d.ts",
  "version": "1.9.3",
  "engines": {
    "node": "> 6.0.0"
  },
  "bin": {
    "bvd": "lib/src/ScriptRunner.js"
  },
  "files": [
    "lib/src/*.js",
    "lib/src/*.d.ts"
  ],
  "nyc": {
    "exclude": [
      "lib/test/**"
    ],
    "reporter": [
      "json",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bespoken/virtual-device-sdk.git"
  },
  "scripts": {
    "lint": "tslint src/**/*.ts && tslint test/**/*.ts",
    "tsc": "tsc",
    "pretest": "npm run tsc && npm run lint",
    "test": "nyc mocha lib/test/*Test.js",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "typings": "typings install",
    "preversion": "npm run tsc"
  },
  "devDependencies": {
    "@types/chai": "^4.0.2",
    "@types/dotenv": "^4.0.0",
    "@types/mocha": "^5.2.0",
    "@types/nock": "^8.2.1",
    "@types/retry": "^0.12.0",
    "@types/sinon": "^2.3.3",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.0.2",
    "codecov": "^3.7.1",
    "mocha": "^5.2.0",
    "nock": "^9.0.24",
    "nyc": "^10.1.2",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.14.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "chalk": "^2.3.1",
    "dotenv": "^4.0.0",
    "https-proxy-agent": "^5.0.0",
    "retry": "^0.12.0"
  }
}
